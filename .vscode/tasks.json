{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Compile AmFn Batch Process",
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Clean AmFn Batch Process",
			"type": "cargo",
			"subcommand": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		}
	]
}